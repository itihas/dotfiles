! Zenburn color-scheme
*.background              :#1f1f1f
*.foreground              :#dcdccc

! black + red
!*.color0                 :#3f3f3f
*.color0                  :#1f1f1f
*.color1                  :#705050

! green + yellow
*.color2                  :#60b48a
*.color3                  :#dfaf8f

! blue + purple
*.color4                  :#506070
*.color5                  :#dc8cc3

! cyan + white
*.color6                  :#8cd0d3
*.color7                  :#dcdccc

! bright-black + bright-red
*.color8                  :#709080
*.color9                  :#dca3a3

! bright-green + bright-yellow
*.color10                 :#c3bf9f
*.color11                 :#f0dfaf

! bright-blue + bright-purple
*.color12                 :#94bff3
*.color13                 :#ec93d3

! bright-cyan + bright-white
*.color14                 :#93e0e3
*.color15                 :#ffffff

*.perl-ext-common: default,matcher
*.url-launcher: /usr/bin/xdg-open
*.matcher.button: 1

*.scrollBar: false
URxvt.font: xft:DejaVu Sans Mono for Powerline:pixelsize=14

! URxvt.perl-ext-common: default, matcher
URxvt.url-launcher:        /usr/bin/xdg-open
! URxvt.matcher.button: 1
